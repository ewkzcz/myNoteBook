@article{参考文献3,
	title = {基于聚类分析与欧氏距离模型的碎纸片拼接复原},
	issn = {2095-5650},
	url = {https://kns.cnki.net/kcms/detail/detail.aspx?dbcode=CJFD&dbname=CJFDLAST2020&filename=DZRU202018071&uniplatform=NZKPT&v=vwj5CCu_UCB2txsN6Q1ZUPLnUrjrlA2m6gsP2IlqFh0H1E13Mq-De0u4iXprm23B},
	abstract = {本文基于相关性分析与图像匹配算法结合系统聚类法与欧氏距离数学模型研究碎纸片的拼装复原问题。首先使用Matlab编程提取出图片关键位置的灰度值矩阵,然后利用spss软件进行图片边缘矩阵的相关分析,得到了各图片边缘特征的连接关系,找出与各矩阵最相似的对应矩阵,得到了各图片最佳匹配对象及相关系数表,最后建立相关性分析在匹配聚类分析的系统聚类法与欧氏距离数学模型,通过图片特征进行匹配排序,利用Matlab绘制出了各图片文字拼接结果。},
	pages = {145--146},
	number = {18},
	journaltitle = {电子技术与软件工程},
	author = {谢亚旗，缪杨，梁伟，王韵，安秋平},
	urldate = {2022-08-25},
	date = {2020},
	keywords = {灰度值矩阵, 聚类分析, 欧氏距离模型, 拼接, 人工干预},
}